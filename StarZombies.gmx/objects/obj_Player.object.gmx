    else if(!place_meeting(x+16,y+11,obj_PlayerCollidable)){
        while(place_meeting(x,y,obj_PlayerCollidable)){
            y += 1;
            x += 1;
        }
    }
    else{
        while(place_meeting(x,y,obj_PlayerCollidable)){
            y -= 1;
            x -= 1;
        } 
    }
}
if (place_meeting(x+hsp,y,obj_PlayerCollidable))
{
