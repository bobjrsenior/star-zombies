a = keyboard_check_direct(ord('A'));
s = keyboard_check_direct(ord('S'));
d = keyboard_check_direct(ord('D'));

x_change = obj_Cursor.x - x;
y_change = obj_Cursor.y - y;

if(x_change == 0){
    if(y_change &gt; 0){
        angle = 90;
    }
    else{
