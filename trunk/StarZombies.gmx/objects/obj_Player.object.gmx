    }
}
else if(y_change == 0){
    if(x_change &gt; 0){
        angle = 0;
    }
    else{
        angle = 180;
    }
}
else{
    angle = 180 * (pi / ((1/(tan((obj_Cursor.y - y) / (obj_Cursor.x - x))))));
