        angle += 360; 
    }
    while(angle &gt; 360){
        angle -= 360;
    }
}

image_angle = -angle;

if(w){
    vsp = -4;
}
else if(s){
    vsp = 4;
}
else{
    vsp = 0;
}
if(a){
    hsp = -4;
}
else if(d){
    hsp = 4;
}
else{
    hsp = 0;
}

x += hsp;
y += vsp;
</string>
